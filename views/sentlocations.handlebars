<div class="container-fluid">
  <p><a href="/" class="btn btn-primary"><span class="glyphicon glyphicon-home"></span> Home</a></p>
  <table class="table table-bordered">
    <tr>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Message</th>
    </tr>
    {{# each items }}
    <tr>
      <td>{{ this.location.lat}}</td>
      <td>{{ this.location.lng}}</td>
      <td>{{ this.location.message}}</td>
    </tr>
    {{/each}}
  </table>
</div>
